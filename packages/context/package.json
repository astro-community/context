{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@astropub/context",
	"description": "Create and share context across Astro components",
	"version": "0.1.0",
	"type": "module",
	"license": "MIT-0",
	"exports": {
		".": {
			"default": "./context.js",
			"types": "./context.d.ts"
		},
		"./context": {
			"default": "./context.js",
			"types": "./context.d.ts"
		},
		"./package.json": "./package.json"
